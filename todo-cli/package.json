  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "standard": "^17.1.0"
  },
  "dependencies": {
    "minimist": "^1.2.8"
  }
}
