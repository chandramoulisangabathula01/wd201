<div class="">
  <!-- Display the count of overdue items -->
  <h5>Overdue</h5>
  <p id="count-overdue"><%= overdue.length %></p>
  <ul class="list-none">
    <!-- Loop through overdue items and include todo.ejs for each -->
    <% for (let i = 0; i < overdue.length; i++) { %>
      <%- include("todo.ejs", { data: overdue[i].dataValues }) %>
    <% } %>
  </ul>

